{
  "name": "buexplain/bitmap",
  "description": "基于 https://github.com/buexplain/roaring 实现的bitmap类",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "buexplain",
      "email": "buexplain@qq.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "buexplain/roaring": "v2.0.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.88",
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "BitMap\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BitMapTest\\": "tests/"
    }
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix ./src",
    "test": "phpunit --configuration phpunit.xml --colors=always"
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.org"
    }
  }
}
